{

  "manifest_version": 2,
  "name": "Youtube_Twitch_Lock",
  "version": "1.0",

  "description": "Closes youtube and twitch on attempted open",

  "icons": {
    "48": "icons/lock-48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["YoutubeTwitchLock.js"]
    }
  ]

}